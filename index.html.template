        /*
        |----------------------------------------------------
        | MAIN
        |----------------------------------------------------
        */

        body {
            font-family: 'Muli', 'Roboto', sans-serif;
            font-size: 16px;
            background: #eee;
        }

        * {
            padding: 0;
            margin: 0;
            box-sizing: border-box;
        }

        a {
            text-decoration: none;
        }

        .container {
            display: block;
            width: 600px;
            margin: 0 auto;
        }

        /*
        |-----------------------------------------------------
        | Buttons
        |-----------------------------------------------------
        */

        a.btn {
            display: inline-block;
            padding: 0.5rem 1rem;
            font-size: 0.875rem;
            color: #4d4d4d;
            background-color: #e9bd13;
            border-radius: 0.3125rem;
            font-weight: 600;
        }

        a.btn.btn-hero {
            font-size: 1.125rem;
            padding: 0.5rem 2rem;
        }

        a.btn.btn-primary {
            background: #197ed1;
            color: #fff;
        }

        a.btn.btn-icon {
            display: inline-flex;
            align-items: center;
            padding-right: 1rem;
        }

        a.btn.btn-icon .icon {
            margin-left: 0.5rem;
        }

        a.btn.btn-icon .icon img {
            width: 24px;
        }

        /*
        |-----------------------------------------------------
        | Blocks
        |-----------------------------------------------------
        */
        .block .block-header {
            padding: 0.5rem 1rem;
            background: #e6e6e6;
        }

        .block .block-header .block-title {
            font-size: 1rem;
            font-weight: 600;
            color: #666666;
        }

        .block .block-content {
            background: #f2f2f2;
        }

        /*
        |-----------------------------------------------------
        | Grid
        |-----------------------------------------------------
        */

        .grid {
            display: flex;
            margin: -0.5rem;
            align-items: flex-start;
        }

        .grid .column {
            width: 50%;
            flex: 1;
            padding: 0.5rem;
        }

        /*
        |-----------------------------------------------------
        | HEADER
        |-----------------------------------------------------
        */

        header {
            background: #fff;
            padding: 1rem 2rem;
        }

        header div.brand {
            text-align: center;
            margin-bottom: 1rem;
        }

        header div.brand .logo {
           text-align: center;
           line-height: 40px;
           vertical-align: middle;
           margin: 0 auto;
        }

        header div.brand .logo img {
            height: 40px;
            margin-right: 8px;
        }

        header div.brand .logo .text {
            color: #333333;
            font-size: 1.125rem;
            line-height: 40px;
            vertical-align: middle;
            font-weight: 300;
        }

        header .navbar ul{
            text-align: center;
            list-style-type: none;
        }

        header .navbar ul li {
            display: inline;
        }

        header .navbar ul li a {
            color: #999999;
            padding: 0.5rem 1rem;
            text-align: center;
            text-transform: uppercase;
            font-weight: 700;
            font-size: 0.875rem;
        }

        header .navbar ul li a:hover {
            color: #e9bd13;
        }

        /*
        |-----------------------------------------------------
        | HERO
        |-----------------------------------------------------
        */
        #hero {
            height: 360px;
            background-image: url('<%host_url%>/assets/images/hero_bg.png');
            background-position: center;
            background-size: cover;
        }

        #hero .hero-content {
            background: rgba(0,0,0,0.55);
            height: 100%;
            color: #fff;
            padding: 1rem 2rem;
        }

        #hero .hero-content > div {
            margin-top: 3.5rem;
        }

        #hero .hero-content h1.title {
            font-size: 2rem;
            font-weight: 400;
            margin-bottom: 1rem;
            line-height: 1.25;
        }

        #hero .hero-content .lead {
            color: #fff;
            font-size: 1rem;
            font-weight: 300;
            margin-bottom: 2rem;
        }

        /*
        |-----------------------------------------------------
        | COURSES
        |-----------------------------------------------------
        */
        #courses {
            background: #fff;
            padding: 1rem 2rem;
        }

        #courses h1.title {
            font-size: 2rem;
            color: #00264b;
            font-weight: 400;
            margin-bottom: 1rem;
            text-align: center;
        }

        #courses .grid .column .image {
            margin-bottom: 1rem;
        }

        #courses .grid .column .image img {
            width: 100%;
        }

        #courses .grid .column h2.title {
            font-size: 1.25rem;
            font-weight: 600;
            margin-bottom: 0.75rem;
            color: #00264b;
        }

        #courses .grid .column .summary {
            font-size: 0.875rem;
            font-weight: 300;
            color: #808080;
            margin-bottom: 1rem;
        }

        #courses .grid .column .btn {
            margin-bottom: 1rem;
        }

        /*
        |-----------------------------------------------------
        | BATCHES
        |-----------------------------------------------------
        */
        .schedule {
            display: flex;
            align-items: center;
            padding: 0.875rem;
        }

        .schedule .icon {
            /* padding: 0.25rem; */
            margin-right: 0.5rem;
        }

        .schedule .icon img {
            width: 40px;
        }

        .schedule .content {
            flex: 1;
        }

        .schedule .content h4.sub-title {
            font-size: 0.875rem;
            font-weight: 700;
            color: #999999;
            text-transform: uppercase;
        }

        .schedule .content .date {
            font-size: 1.25rem;
            font-weight: 600;
            color: #4d4d4d;
        }

        /*
        |-----------------------------------------------------
        | FOOTER
        |-----------------------------------------------------
        */
        footer {
            background: #00264b;
            padding: 2rem;
            color: #fff !important;
        }

        footer .address {
            font-size: 1rem;
            font-weight: 400;
            margin-bottom: 1rem;
        }

        footer .contacts {
            font-size: 0.75rem;
            font-weight: 300;
            margin-bottom: 1rem;
        }

        footer .social-icons {
            display: flex;
            align-items: center;
        }

        footer .social-icons a.social-icon{
            opacity: 0.875;
        }

        footer .social-icons a.social-icon:hover {
            opacity: 1;
        }

        footer .social-icons a.social-icon:not(:last-child) {
            margin-right: 0.5rem;
        }

        footer .social-icons a.social-icon img{
            height: 0.875rem;
        }
